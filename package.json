{
  "name": "discord-trakt-lists-movies-notifier",
  "version": "1.0.0",
  "description": "A discord bot who notify when movies stored in Trakt lists, are coming soon.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "tsc -p . && node -r dotenv/config ./dist/index.js dotenv_config_path=deploy/.env",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "trakt",
    "discord",
    "bot"
  ],
  "author": "thibaultherve",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "discord.js": "^13.8.0",
    "moment": "^2.29.3",
    "sql-template-strings": "~2.2.2",
    "sqlite": "~4.0.19",
    "sqlite3": "~5.0.2",
    "winston": "^3.7.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
